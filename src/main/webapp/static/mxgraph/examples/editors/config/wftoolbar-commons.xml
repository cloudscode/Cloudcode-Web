	<add as="connect" mode="connect"
		icon="../static/mxgraph/examples/editors/images/straight.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'noEdgeStyle=1';
			}
		}
	]]></add>
	<add as="connect" mode="connect"
		icon="../static/mxgraph/examples/editors/images/connect.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'edgeStyle=segmentEdgeStyle';
			}
		}
	]]></add>
	<add as="connect" mode="connect"
		icon="../static/mxgraph/examples/editors/images/arrow.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'arrowEdge';
			}
		}
	]]></add>
